{
  "name": "web-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "copy-blueprints": "copyfiles -u 3 \"../Generated/BlueprintImages/*\" public/images/blueprint-images/",
    "copy-card-images": "copyfiles -u 3 \"../Generated/CardImages/*\" public/images/card-images/",
    "copy-card-images-raw": "copyfiles -u 3 \"../CardResources/CardImagesRaw/*\" public/images/card-images-raw/",
    "copy-layout-elements": "copyfiles -u 3 \"../CardResources/CardLayoutElements/*\" public/images/card-layout-elements/",
    "copy-tooltips": "copyfiles -u 3 \"../Generated/CardTooltips/*\" public/tooltips/card-tooltips/",
    "copy-assets": "npm run copy-blueprints && npm run copy-card-images && npm run copy-card-images-raw && npm run copy-layout-elements && npm run copy-tooltips",
    "predev": "npm run copy-assets",
    "dev": "vite",
    "prebuild": "npm run copy-assets",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "use-sound": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "vite": "^6.3.5"
  }
}
